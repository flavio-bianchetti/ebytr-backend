    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "node": "^17.7.2",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
